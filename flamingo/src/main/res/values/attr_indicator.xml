<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Indicator">
        <attr name="ds_i_size" format="enum">
            <enum name="big" value="12" />
            <enum name="small" value="8" />
        </attr>
        <attr name="ds_i_color" format="enum">
            <enum name="primary" value="1" />
            <enum name="error" value="2" />
            <enum name="secondary" value="3" />
            <enum name="warning" value="4" />
        </attr>
    </declare-styleable>
</resources>
